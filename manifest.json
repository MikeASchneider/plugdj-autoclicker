{
	"manifest_version": 2,
	"name": "Plug DJ Auto-clicker",
	"version": "0.1",
	"content_scripts": [
		{
			"matches": ["http://plug.dj/*", "https://plug.dj/*"],
			"js": ["script.js"]
		}
	],
	"permissions": [
		"tabs", "http://plug.dj/*", "https://plug.dj/*"
	],
	"web_accessible_resources": ["script.js"]
}